<Window x:Class="Music_Downloader.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        xmlns:local="clr-namespace:Music_Downloader"
        mc:Ignorable="d"
        Title="Netease Music Downloader" Height="450" Width="800">
    <Grid>

        <TextBox x:Name="id" HorizontalAlignment="Left" Margin="503,103,0,0" Text="1296548028" TextWrapping="Wrap" VerticalAlignment="Top" Width="134" Height="37" CaretBrush="#FF39C5BB" FontSize="22" SelectionBrush="#FF39C5BB" OpacityMask="#FF39C5BB"/>
        <Button x:Name="button" Content="Download" HorizontalAlignment="Center" Margin="0,281,0,0" VerticalAlignment="Top" Height="68" Width="172" Background="#FF39C5BB" BorderBrush="#FF39C5BB" FontSize="24" Click="button_Click"/>
        <Label x:Name="label" Content="https://music.163.com/#/song?id=" HorizontalAlignment="Left" Margin="98,101,0,0" VerticalAlignment="Top" Width="400" FontSize="24" BorderBrush="#FF39C5BB" OpacityMask="#FF39C5BB" Foreground="#FF39C5BB"/>
    </Grid>
</Window>
